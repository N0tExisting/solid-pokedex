{
	"cleanUrls": true,
	"rewrites": [
		{
			"source": ".*",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/static/(.*)",
			"headers" : [
				{
					"key" : "Cache-Control",
					"value" : "public, max-age=31536000, must-revalidate"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers" : [
				{
					"key" : "X-Content-Type-Options",
					"value" : "nosniff"
				},
				{
					"key" : "X-Frame-Options",
					"value" : "DENY"
				}
			]
		}
	]
}
